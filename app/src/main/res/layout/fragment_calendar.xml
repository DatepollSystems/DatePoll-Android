<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.datepollsystems.datepoll.ui.main.CalendarFragment"
    android:id="@+id/frameLayout2">

    <com.applandeo.materialcalendarview.CalendarView
        style="@style/calendar"
        android:id="@+id/calendarView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:abbreviationsBarColor="@color/primaryLightColor"
        app:abbreviationsLabelsColor="@color/colorWhite"
        app:anotherMonthsDaysLabelsColor="@color/primaryDarkColor"
        app:disabledDaysLabelsColor="@color/primaryLightColor"
        app:eventsEnabled="true"
        app:headerColor="@color/primaryDarkColor"
        app:headerLabelColor="@color/colorWhite"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:selectionColor="@color/primaryDarkColor"
        app:selectionLabelColor="@color/primaryDarkColor"
        app:todayLabelColor="@color/secondaryDarkColor" />

</androidx.constraintlayout.widget.ConstraintLayout>
